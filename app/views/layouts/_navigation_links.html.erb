<%= link_to '<li>Home</li>'.html_safe, root_path %>
<% if user_signed_in? %>
  <%= link_to '<li>Dashboard</li>'.html_safe, users_dashboard_path %>
  <%= link_to '<li>Edit Account</li>'.html_safe, edit_user_registration_path %>
<% else %>
  <%= link_to '<li>Sign up</li>'.html_safe, new_user_registration_path %>
<% end %>
<% if user_signed_in? %>
  <%= link_to '<li class="menu-item-last">Logout</li>'.html_safe, destroy_user_session_path, :method=>'delete' %>
<% else %>
  <%= link_to '<li class="menu-item-last">Login</li>'.html_safe, new_user_session_path %>
<% end %>
